{{#data.entries}}
<h1>{{category}}</h1>
<table class="table table-hover">
  <tbody>
    <tr>
    <th>Created</th>
    <th>Last Changed</th>
    <th>Valid Until</th>
    <th>Entry</th>
    {{#isLevel0}}
      <th>Level 0</td>
    {{/isLevel0}}
    {{#isLevel1}}
      <th>Level 1</th>
    {{/isLevel1}}
    {{#isLevel2}}
      <th>Level 2</th>
    {{/isLevel2}}
    {{#isLevel3}}
      <th>Level 3</th>
    {{/isLevel3}}
    {{#isLevel4}}
      <th>Level 4</th>
    {{/isLevel4}}
    {{#isLevel5}}
      <th>Level 5</th>
    {{/isLevel5}}
    {{#isLevel6}}
      <th>Level 6</th>
    {{/isLevel6}}
    {{#isLevel7}}
      <th>Level 7</th>
    {{/isLevel7}}
    <tr>
  {{#dataEntries}}{{#dataRows}}
    <tr>
    <td>{{created}}</td>
    <td>{{lastChanged}}</td>
    <td>{{validTo}}</td>
    <td>{{entry}}</td>
    {{#isLevel0}}
      <td>{{#level0}}{{level0}}{{/level0}}</td>
    {{/isLevel0}}
    {{#isLevel1}}
      <td>{{#level1}}{{level1}}{{/level1}}</td>
    {{/isLevel1}}
    {{#isLevel2}}
      <td>{{#level2}}{{level2}}{{/level2}}</td>
    {{/isLevel2}}
    {{#isLevel3}}
      <td>{{#level3}}{{level3}}{{/level3}}</td>
    {{/isLevel3}}
    {{#isLevel4}}
      <td>{{#level4}}{{level4}}{{/level4}}</td>
    {{/isLevel4}}
    {{#isLevel5}}
      <td>{{#level5}}{{level5}}{{/level5}}</td>
    {{/isLevel5}}
    {{#isLevel6}}
      <td>{{#level6}}{{level6}}{{/level6}}</td>
    {{/isLevel6}}
    {{#isLevel7}}
      <td>{{#level7}}{{level7}}{{/level7}}</td>
    {{/isLevel7}}
    </tr>
  {{/dataRows}}{{/dataEntries}}
  </tbody>
</table>
{{/data.entries}}